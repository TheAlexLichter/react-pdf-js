{
  "name": "@mikecousins/react-pdf",
  "version": "5.5.1",
  "description": "Simple React component to wrap up PDF.js. The easiest way to render PDFs in your React app.",
  "author": "mikecousins",
  "license": "MIT",
  "repository": "mikecousins/react-pdf-js",
  "files": [
    "dist"
  ],
  "main": "./dist/mikecousins-react-pdf.umd.js",
  "module": "./dist/mikecousins-react-pdf.es.js",
  "types": "./dist/mikecousins-react-pdf-es.d.ts",
  "exports": {
    ".": {
      "import": "./dist/mikecousins-react-pdf.es.js",
      "require": "./dist/mikecousins-react-pdf.umd.js"
    }
  },
  "scripts": {
    "build": "vite build",
    "lint": "eslint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "jest": {
    "setupFiles": [
      "jest-canvas-mock"
    ]
  },
  "devDependencies": {
    "@testing-library/react": "13.3.0",
    "@types/jest": "28.1.4",
    "@types/react": "18.0.15",
    "@types/react-dom": "18.0.6",
    "@vitejs/plugin-react": "1.3.2",
    "jest-canvas-mock": "^2.4.0",
    "typescript": "4.7.4",
    "vite": "2.9.14",
    "vite-plugin-dts": "1.2.1"
  },
  "dependencies": {
    "pdfjs-dist": "2.14.305"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
